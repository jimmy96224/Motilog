<% if @logs.present? %>
  <% @logs.each do |log| %>
      <%= link_to user_instrument_path(user_id: current_user.id, id: log.instrument.id) do %>
          <%= image_tag log.instrument.get_instrument_image(100,100) %>
          <%= log.instrument.name %>
          <%= log.date %>
          <%= log.log %>
     <% end %>
  <% end %>
  <% else %>
  ログはありません
<% end %>
